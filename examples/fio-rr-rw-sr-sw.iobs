[global]
device=/dev/sda
repetition=3
runtime=10
schedulers=cfq,noop,deadline
delay=1
workload=fio

[fio-sequential-read]
command=fio examples/fio/fio-seq-read.job

[fio-sequential-write]
command=fio examples/fio/fio-seq-write.job

[fio-random-read]
command=fio examples/fio/fio-rand-read.job

[fio-random-write]
command=fio examples/fio/fio-rand-write.job